<?xml version="1.0"?>
<as3lib:MxmlEventDispatcher xmlns:fx="http://ns.adobe.com/mxml/2009"
                            xmlns:ap="org.apache.flex.collections.*"
                            xmlns:as3lib="com.flashflexpro.as3lib.utils.*" xmlns:solr="com.customshow.solr.*">
    <fx:Script><![CDATA[
        [Bindable]
        public var searchModel:SearchModel;

        public function getAllFields():ArrayList{
            return ___allFields;
        }
        ]]></fx:Script>
    <fx:Declarations>
        <ap:ArrayList id="___allFields">
            <solr:QueryFieldT id="uid" name="id" label="id" searchModel="{searchModel}" isArray="false" isStrID="true"/>
        </ap:ArrayList>
    </fx:Declarations>
</as3lib:MxmlEventDispatcher>
